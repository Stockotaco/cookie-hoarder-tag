const getCookie = require("getCookieValues");
const getContainerVersion = require("getContainerVersion");
const getUrl = require("getUrl");
const getQueryParameters = require("getQueryParameters");
const setCookie = require("setCookie");
const consoleLog = require("logToConsole");
const makeString = require("makeString");

let fail = false;

function log(input) {
  if (getContainerVersion().previewMode) {
    consoleLog(input);
  }
}

// function to create cookie
function cookieHoard(name, value) {
  var options = {
    domain: data.cookieDomain,
    path: data.cookiePath,
    "max-age": data.cookieExpiration * 2592000,
    secure: true,
  };

  if (data.cookieOverride) {
    // check if value exists. If it does, exit.
    if (getCookie(data.cookiePrefix +name) != "") {
      log("cookie already exists and it will not be overruled");
      return;
    } else {
      // If value is not found - write new cookies
      if (data.cookiePrefix != undefined) {
        setCookie(data.cookiePrefix + name, value, options);
        log(data.cookiePrefix + name + " cookie set with a value of " + value);
      } else {
        setCookie(name, value, options);
        log(name + " cookie set with a value of " + value);
      }
    }
  } else {
    // Write Cookies - will override existing values
    if (data.cookiePrefix != undefined) {
      setCookie(data.cookiePrefix + name, value, options);
      log(data.cookiePrefix + name + " cookie set with a value of " + value);
    } else {
      setCookie(name, value, options);
      log(name + " cookie set with a value of " + value);
    }
  }
}

// Assign all search params to a value
const queryString = getUrl("query");
// log("queryString: " + queryString + " - " + typeof queryString);
const queryParams = queryString.length ? queryString.split("&") : [];
// log("queryParams: " + queryParams + " - " + typeof queryParams);

var approvedParamsString = data.parametersToStore;
log(
  "Approved Parameters: " +
    approvedParamsString +
    " - " +
    typeof approvedParamsString
);
// Split string up into an object
var approvedParams = approvedParamsString.split(",");

// Check if search param is in approvevd list
queryParams.forEach(function (key) {
  key = key.split("=");
  if (approvedParams.indexOf(key[0]) != "-1") {
    // Match was found
    log("key: " + key[0] + " value: " + key[1] + " - approved");
    cookieHoard(key[0], key[1]);
  } else {
    // Match was not found
    log("key: " + key[0] + " value: " + key[1] + " - denied");
  }
});

var sourceHistory = getCookie("sourceHistory");
var histories = makeString(sourceHistory).split("|");
var lastIndex = histories.length - 1;
// log("lastIndex: " + lastIndex + "-" + histories[lastIndex]);

if (data.sourceHistoryOption) {
  var sourceHistoryOptions = {
    domain: data.cookieDomain,
    path: data.cookiePath,
    "max-age": 63072000, // 2 years-ish
    secure: true,
  };
  // log("Current Source History: " + '"' + sourceHistory + '"');
  var newSource = getQueryParameters("utm_source");
  if (histories[lastIndex] == newSource) {
    // do nothhing
    log("new source matches last entry - do not append");
  } else {
    if (sourceHistory != "") {
      // append new source with |
      setCookie(
        "sourceHistory",
        sourceHistory + "|" + newSource,
        sourceHistoryOptions
      );
      log(newSource + " added to sourceHistory cookie");
    } else {
      // set sourceHistory for first time
      setCookie("sourceHistory", newSource, sourceHistoryOptions);
      log("sourceHistory cookie set with value " + newSource);
    }
  }
}

data.gtmOnSuccess();


